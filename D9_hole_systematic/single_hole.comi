CYLINDER Name='Cylinder2' X0=#D1 Y0=#PX_Y Z0=#PX_Z X1=#D2 Y1=#PX_Y Z1=#PX_Z -TUBE SHAPECONTROL=SIMPLE MAJORRADIUS=#DX/2 MINORRADIUS=#DX/2 TOPRADIUS=#DX/2 SIDES=2
PICK OPTION=TOGGLE, | PICK PROPERTY=Name LABEL=L2_Out
PICK OPTION=TOGGLE, | PICK PROPERTY=Name LABEL=Cylinder2
COMBINE OPERATION=SUBTRACT +REGULAR
$END DO


//--- DO LOOP FOR Y HOLES
/$DO #IND 0 1 1
/$IF #IND EQ 0
/ VARIABLE OPTION=PARAMETER, NAME=#D2 VALUE=#D_out
