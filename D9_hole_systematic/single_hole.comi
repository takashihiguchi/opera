/CYLINDER Name='Cylinder3' X0=#PY_X Y0=#D1 Z0=#PY_Z X1=#PY_X Y1=#D2 Z1=#PY_Z -TUBE SHAPECONTROL=SIMPLE MAJORRADIUS=#DY/2 MINORRADIUS=#DY/2 TOPRADIUS=#DY/2 SIDES=2
/PICK OPTION=TOGGLE, | PICK PROPERTY=Name LABEL=L3_Out
/PICK OPTION=TOGGLE, | PICK PROPERTY=Name LABEL=Cylinder3
