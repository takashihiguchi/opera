/ VARIABLE OPTION=PARAMETER, NAME=#D2 VALUE=#D_out
/ VARIABLE OPTION=PARAMETER, NAME=#D1 VALUE=#D_in
/$ELIF #IND EQ 1
/ VARIABLE OPTION=PARAMETER, NAME=#D2 VALUE=-#D_out 
